<resources xmlns:tools="http://schemas.android.com/tools">
    <!-- Primary brand color. -->

    <style name="Theme.myZipZap.Dark" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
    <!-- Primary brand color. -->
    <item name="colorPrimary">@color/prussianSea</item>
    <item name="colorPrimaryVariant">@color/ceruleanPool</item>
    <item name="colorOnPrimary">@color/white</item>
    <!-- Secondary brand color. -->
    <item name="colorSecondary">@color/lightBlue</item>
    <item name="colorSecondaryVariant">@color/superLightBlue</item>
    <item name="colorOnSecondary">@color/black</item>
    <!-- Status bar color. -->
    <item name="android:statusBarColor" tools:targetApi="l">@color/prussianSea</item>
        <item name="android:navigationBarColor" >@color/prussianSea</item>
    <!-- Customize your theme here. -->
</style>
    <style name="Theme.myZipZap.Dark.noActionBar" parent="Theme.MaterialComponents.DayNight.DarkActionBar">
        <!-- Primary brand color. -->
        <item name="colorPrimary">#011F2C</item>
        <item name="colorPrimaryVariant">#004055</item>
        <item name="colorOnPrimary">@color/white</item>
        <!-- Secondary brand color. -->
        <item name="colorSecondary">#043339</item>
        <item name="colorSecondaryVariant">#035C61</item>
        <item name="colorOnSecondary">@color/lightBlue</item>
        <!-- Status bar color. -->
        <item name="android:statusBarColor" tools:targetApi="l">@color/prussianSea</item>
        <item name="android:navigationBarColor" >@color/prussianSea</item>
        <!-- Customize your theme here. -->
    </style>
    <style name="NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>

    </style>
    <!-- Customize your theme here. -->
</resources>